<?xml version='1.0' encoding='utf-8'?>
<widget id="net.marcellozaniboni.sonqui" version="0.9.10" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>sonqui</name>
    <description>
        Copy or share your geolocation quickly.
    </description>
    <author email="marcello.zaniboni@gmail.com" href="http://www.marcellozaniboni.net">
        Marcello Zaniboni
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="www/res/icons/android/ldpi.png" density="ldpi" />
        <icon src="www/res/icons/android/mdpi.png" density="mdpi" />
        <icon src="www/res/icons/android/hdpi.png" density="hdpi" />
        <icon src="www/res/icons/android/xhdpi.png" density="xhdpi" />
        <icon src="www/res/icons/android/xxhdpi.png" density="xxhdpi" />
        <icon src="www/res/icons/android/xxxhdpi.png" density="xxxhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
